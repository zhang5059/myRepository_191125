.orderBox {
    width: 100%;
    height: 100%;
    position: absolute;
    background: #F3F4F5;
}
.orderTitle {
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 40rpx;
    display: flex;
    align-items: center;
    background: #F3F4F5;
    color: #666;
    font-size: 28rpx;
    position: fixed;
    top: 0;
}
.conList {
    margin-top: 78rpx;
    margin-bottom: 125rpx;
}
.orderContent {
    width: 100%;
    height: 370rpx;
    box-sizing: border-box;
    padding: 20rpx 20rpx 20rpx 40rpx;
    background: #fff;
    border-bottom: 12rpx solid #EFEFF0;
}
.orderContent:last-child {
    border-bottom: 2rpx solid #eee;
}
.content1 {
    display: flex;
    justify-content: space-between;
}
.content2>view {
    margin-top: 20rpx;
    display: flex;
    align-items: center;
}

.detailBtn {
    display: flex;
    justify-content: flex-end;
    width: 100%;

}
.detailBtn>view {
    width: 150rpx;
    height: 60rpx;
    background: #07C160;
    border-radius: 10rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
    font-size: 28rpx;
}

.btnkefu {
    width: 100%;
    position: fixed;
    bottom: 10rpx;
    left: 50%;
    margin-left: -50%;
    color: #fff;
}
.btnkefu>button {
    width: 500rpx;
    height: 110rpx;
    background: #07C160;
    color: #fff;
    border-radius: 20rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}


