.map_container {
    position: relative;
}


.sousuo {
    position: fixed;
    z-index: 999;
    background: #fff;
    top: 0;
}

.sousuo {
    width: 100vh;
    height: 90rpx;
    background: #fff;
    display: flex;
    align-items: center;
}

.sousuoInner {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
}

.sousuoLeft {
    width: 10%;
    margin-left: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
}

/* .sousuoLeft>image {
    margin-left: 5rpx;
    margin-top: 5rpx;
} */

.sousuoRight {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 100%;
    width: 40%;
}

