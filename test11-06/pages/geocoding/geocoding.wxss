.bottomBtn {
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 20rpx;
    left: 50%;
    width: 700rpx;
    height: 110rpx;
    border-radius: 20rpx;
    background: #07C160;
    margin-left: -350rpx;
    color: #fff;
    font-size: 38rpx;
}

.bottomBtn>button {
}

.tishi {
    width: 700rpx;
    height: 160rpx;
    background: #fff;
    position: fixed;
    bottom: 145rpx;
    left: 50%;
    margin-left: -350rpx;
    border-radius: 20rpx;
    padding: 20rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.tishiInner1 {
    display: flex;
    align-items: center;
    height: 55%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.tishiInner1>view:nth-child(2) {
    font-size: 34rpx;
    font-weight: bold;
    color: #2b2b2b;
}

.tishidian {
    width: 18rpx;
    height: 18rpx;
    background: #58b096;
    border-radius: 50%;
    margin-right: 20rpx;
}

.tishiInner2>view {
    margin-left: 40rpx;
    color: #68ad98;
    font-size: 26rpx;
}



.sousuoResult {
    margin-top: 90rpx;
}
.resultBox {
    display: flex;
    padding: 25rpx 0;
    padding-left: 40rpx;
    align-items: center;
    border-bottom: 2rpx solid #f4f4f4;
    box-sizing: border-box;
}

.resultContent view {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}